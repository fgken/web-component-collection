{
  "name": "web-component-collection",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "lint": "npx tsfmt --verify -r && npx eslint --ext .js,.mjs,.jsx,.ts,.tsx src/ && tsc",
    "test": "npx jest",
    "test-watch": "npx jest --watchAll",
    "check": "npm run lint && npm run test",
    "fix": "npx tsfmt -r ; npx eslint --fix --ext .js,.mjs,.jsx,.ts,.tsx src/",
    "start": "npx webpack-dev-server --env development --hot",
    "build": "npx webpack-cli --env production"
  },
  "dependencies": {
    "@babel/polyfill": "^7.8.3",
    "@reduxjs/toolkit": "^1.2.4",
    "moment": "^2.24.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "recharts": "^2.0.0-beta.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@types/jest": "^24.9.1",
    "@types/react": "^16.9.18",
    "@types/react-dom": "^16.9.5",
    "@types/react-redux": "^7.1.7",
    "@types/recharts": "^1.8.5",
    "@types/redux": "^3.6.0",
    "@typescript-eslint/eslint-plugin": "^2.17.0",
    "@typescript-eslint/parser": "^2.17.0",
    "babel-loader": "^8.0.6",
    "case-sensitive-paths-webpack-plugin": "^2.3.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.18.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^25.1.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "typescript": "^3.7.5",
    "typescript-formatter": "^7.2.2",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1",
    "webpack-manifest-plugin": "^2.2.0"
  }
}
